<%- current_user?.jobs.each do |job| -%>
	<div class="small-12 columns">
		<%=link_to job.title, job_path(job) %>
	</div>
	<div class="small-12 columns">
		<%= job.description %>
	</div>
	<div class="small-12 columns">
		<%= job.github_repo %>
	</div>
	<div class="small-12 columns">
		<%= job.created_at %>
	</div>
<%- end -%>	
